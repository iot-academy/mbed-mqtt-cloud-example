{
    "config": {
        "hostname": {
            "help": "The demo will try to connect to this web address on port 80 (or port 443 when using tls).",
            "value": "\"ifconfig.io\""
        },
        "use-tls-socket": {
            "value": false
        },
        "socket-bufsize": {
            "help": "Max socket data heap usage",
            "value": "1024"
        },
        "tx": {
            "help": "TX pin for serial connection",
            "value": "UNWD_GPIO_25"
        },
        "rx": {
            "help": "RX pin for serial connection",
            "value": "UNWD_GPIO_26"
        }
    },
    "target_overrides": {
        "*": {
            "target.network-default-interface-type": "WIFI",
            "nsapi.default-wifi-security": "WPA_WPA2",
            "nsapi.default-wifi-ssid": "\"ssid\"",
            "nsapi.default-wifi-password": "\"password\"",
            "platform.stdio-baud-rate": 115200,
            "platform.default-serial-baud-rate": 115200,
            "esp8266.tx": "UNWD_GPIO_25",
            "esp8266.rx": "UNWD_GPIO_26",
            "esp8266.socket-bufsize": "1024",
            "esp8266.debug": false,
            "esp8266.provide-default" : true,
            "mbed-mqtt.max-packet-size": "1024"
        }
    }
}
